<odoo>
    <data>
        
        <template id="shipstation_report_delivery_document2" inherit_id="delivery.report_delivery_document2">
            <xpath expr="//td[3]" position="replace">
                <td t-if="o.picking_type_id.code == 'outgoing' and o.carrier_id">
                    <t t-if="o.ss_carrier">
                        <span t-esc="o.ss_carrier"/>
                    </t>
                    <t t-else="">
                        <span t-field="o.carrier_id"/>
                    </t>
                </td>
            </xpath>
        </template>

    </data>
</odoo>