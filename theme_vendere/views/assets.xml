<odoo>
	<data>

		<template id="vendere_assets" name="Vendere Styles" inherit_id="website.assets_frontend">
            <xpath expr="link[last()]" position="after">
                <link rel="stylesheet" type="text/scss" href="/theme_vendere/static/scss/navbar.scss"/>
                <link rel="stylesheet" type="text/scss" href="/theme_vendere/static/scss/footer.scss"/>
                <link rel="stylesheet" type="text/scss" href="/theme_vendere/static/scss/col-2dot4.scss"/>
                <link rel="stylesheet" type="text/scss" href="/theme_vendere/static/scss/products-list.scss"/>
                <link rel="stylesheet" type="text/scss" href="/theme_vendere/static/scss/quickview.scss"/>
                <link rel="stylesheet" type="text/scss" href="/theme_vendere/static/scss/product.scss"/>
                <link rel="stylesheet" type="text/scss" href="/theme_vendere/static/scss/snippets.scss"/>
                <link rel="stylesheet" type="text/css" href="/theme_vendere/static/scss/lightslider.css"/>
                <link rel="stylesheet" type="text/css" href="/theme_vendere/static/scss/contact.scss"/>
                <script type="text/javascript" src="/theme_vendere/static/js/products-quickview.js"></script>
                <script type="text/javascript" src="/theme_vendere/static/js/products-view.js"></script>
                <script type="text/javascript" src="/theme_vendere/static/js/navbar.js"></script>
                <script type="text/javascript" src="/theme_vendere/static/js/featured_products.js"/>
                <script type="text/javascript" src="/theme_vendere/static/js/checkout.js"/>
            </xpath>
        </template>

        <template id="snippet_js" inherit_id="website.assets_editor" name="Snippet JS">
            <xpath expr="script[last()]" position="after">
                <script type="text/javascript" src="/theme_vendere/static/js/featured_categories.js"/>
            </xpath>
        </template>

        <template id="body_script" inherit_id="website.layout">
            <xpath expr="//div[@id='wrapwrap']" position="after">
                <script type="text/javascript" src="/theme_vendere/static/js/lightslider.js"></script>
                <script type="text/javascript">
                    $(document).ready(function(){
                      $('.slider-products-container').lightSlider({
                        item: 5,
                        autoWidth: true,
                        pager: false,
                      });
                    });
                </script>
            </xpath>
        </template>

	</data>
</odoo>