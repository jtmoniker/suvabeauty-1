<odoo>
  <data>
    
    <record id="res_config_settings_view_form" model="ir.ui.view">
        <field name="name">res.config.settings.view.form.account.taxcloud.warehouse</field>
        <field name="model">res.config.settings</field>
        <field name="inherit_id" ref="account.res_config_settings_view_form"/>
        <field name="arch" type="xml">
          <div name="account_taxcloud_right_pane" position="inside">
            <div class="content-group" attrs="{'invisible': [('module_account_taxcloud', '=', False)]}">
              <div class="row">
                  <label string="Remote Origin" for="taxcloud_remote_origin" class="col-lg-3 o_light_label" />
                  <field name="taxcloud_remote_origin" class="oe_inline"/>
              </div>
            </div>
          </div>
        </field>
    </record>
  </data>
</odoo>